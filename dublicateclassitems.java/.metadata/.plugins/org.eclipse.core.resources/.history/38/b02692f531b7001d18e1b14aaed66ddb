package var.arg.method;

public class EvenNo {
	
	
	public static void main(String[]args) {
	
	int[]a= {21,22,23,24,25,26};
	int sum=0;
	int i=0;
	int len=a.length ;
			
	
			
			
			
				if(a[i]%2==0) {
					
					System.out.println("Even no");		
				}
					
				else {
					
					System.out.println("odd no");	
					
					System.out.println(a[i]);	
					sum=sum+a[i];
				}
				
				i++;
				
				
				
			
			
			
			
			
			
			
	
	
	
	 
		 
		 
		 
		 
	 
	}
