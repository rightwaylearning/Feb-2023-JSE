package var.arg.method;

public class EvenNo {
	
	
	public static void main(String[]args) {
	
	int[]a= {21,22,23,24,25,26};
	int sum=0;
	int i=0;
	int len=a.length ;
			
			System.out.println("Even no");	
			
			
			while(i<len) {
				
				sum=sum+a[i];
				
				
				
			}
			i++;
			
			
			
			
			
	
	
	
	 
		 
		 
		 
		 
	 
	}
}
